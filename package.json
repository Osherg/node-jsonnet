{
  "name": "@hanazuki/node-jsonnet",
  "version": "0.1.1",
  "description": "Jsonnet addon for Node.js",
  "bugs": "https://github.com/hanazuki/node-jsonnet/issues",
  "homepage": "https://github.com/hanazuki/node-jsonnet",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanazuki/node-jsonnet"
  },
  "license": "(MIT AND Apache-2.0)",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "CMakeLists.txt",
    "/lib/*",
    "/types/*.d.ts",
    "/binding/*",
    "/test/*",
    "/jsonnet/CMakeLists.txt.in",
    "/jsonnet/include/*.h",
    "/jsonnet/core/*.c",
    "/jsonnet/core/*.cpp",
    "/jsonnet/core/*.h",
    "/jsonnet/cmd/*.cpp",
    "/jsonnet/cmd/*.h",
    "/jsonnet/cpp/*.cpp",
    "/jsonnet/cpp/*.h",
    "/jsonnet/third_party/json/*.hpp",
    "/jsonnet/third_party/md5/*.cpp",
    "/jsonnet/third_party/md5/*.h",
    "/jsonnet/stdlib/*.cpp",
    "/jsonnet/stdlib/*.jsonnet",
    "/jsonnet/test_suite/*"
  ],
  "keywords": [
    "jsonnet",
    "node-addon"
  ],
  "scripts": {
    "install": "cmake-js build",
    "test": "node test/test.js",
    "cmake-verbose": "cmake-js rebuild -l verbose --CDCMAKE_VERBOSE_MAKEFILE=1"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "cmake-js": "^6.1.0",
    "node-addon-api": "^2.0.0"
  },
  "peerDependencies": {
    "@types/node": "*"
  },
  "devDependencies": {
    "dtslint": "^3.3.0",
    "typedoc": "^0.16.11"
  }
}
